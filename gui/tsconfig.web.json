{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    },
    "types": ["vitest/globals"]
  },
  "include": [
    "src/renderer/src/**/*",
    "src/renderer/*.d.ts"
  ]
}
